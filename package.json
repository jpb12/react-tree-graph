    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/react-webpack5": "^7.6.20",
    "babel-jest": "^29.7.0",
    "enzyme": "^3.11.0",
