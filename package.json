{
  "name": "react-tree-graph",
  "version": "3.1.0",
  "description": "Generates a tree graph from data",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpb12/react-tree-graph.git"
  },
  "keywords": [
    "d3",
    "graph",
    "react",
    "svg",
    "tree",
    "ui"
  ],
  "author": "James Brierley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpb12/react-tree-graph/issues"
  },
  "homepage": "https://jpb12.github.io/react-tree-graph",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^22.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "webpack": "^3.10.0",
    "webpackbar": "^2.6.1"
  },
  "dependencies": {
    "clone": "^2.1.1",
    "core-js": "^2.5.0",
    "d3-ease": "^1.0.3",
    "d3-hierarchy": "^1.1.5",
    "prop-types": "^15.5.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "Components/**/*.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./__tests__/shims.js",
      "./__tests__/startup.js"
    ],
    "testPathIgnorePatterns": [
      ".eslintrc.js",
      "startup.js",
      "shims.js"
    ]
  },
  "peerDependencies": {
    "react": "^15.3 || ^16"
  },
  "scripts": {
    "build": "set NODE_ENV=production && webpack -p --progress",
    "build-dev": "set NODE_ENV=development && webpack --progress",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "eslint": "eslint Components __tests__",
    "test": "jest",
    "version": "npm run build && npm run build-dev"
  }
}
